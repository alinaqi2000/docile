<?php

use Docile\Console\Commands\RunServerCommand;

require __DIR__ . '/vendor/autoload.php';

$serverCommand = new RunServerCommand();
switch ($argv[1]) {
    case 'deliver':
        $serverCommand->execute();
        break;
    case 'train':
        $serverCommand->test();
        break;
    default:
        die("\nInvalid Command!\n\n");
}
