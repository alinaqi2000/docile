{
    "name": "alinaqi2000/docile",
    "description": "The skeleton application for the Docile framework.",
    "type": "project",
    "keywords": [
        "docile",
        "framework"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Docile\\": "framework/",
            "App\\": "app/",
            "Database\\": "database/"
        },
        "files": [
            "framework/Helpers/environment.php",
            "framework/Helpers/codes/error_codes.php",
            "framework/Helpers/codes/success_codes.php",
            "framework/Helpers/dev.php",
            "framework/Helpers/render.php",
            "framework/Helpers/session.php"
        ]
    },
    "autoload-dev": {},
    "authors": [
        {
            "name": "Ali Naqi Al-Musawi",
            "email": "alinaqi2000@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^10.11",
        "vlucas/phpdotenv": "^5.5",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}